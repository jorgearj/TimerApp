{
    "name": "TimerApp",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Simple test Timer App",
    "icons": {
        "24": "resources/images/timer_24.png",
        "128": "resources/images/timer_128.png"
    },
    "offline_enabled": true,
    "sandbox": {
        "pages": ["sandbox.html"]
    },
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "permissions": [
        "http://*/*",
        "https://*/*"
    ]
}